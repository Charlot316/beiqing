.bannerBody{
    width:100%;
    height:100%;
    position: absolute;
    top:0;
    left:0;
    background: rgba(44,44,44,0.2);
    line-height:30px;
    text-align:center;
    
    a{
        color:white;
        margin-right:10px;
        text-decoration:underline;
         &:not(:last-child)::after {
            content: '';
            position: absolute;
            top: 50%;
            transform: translateY(-50%);
            width: 1px;
            height: 15px;
            margin-left:5px;
            background-color: white;
        }
    }
}

   
    
